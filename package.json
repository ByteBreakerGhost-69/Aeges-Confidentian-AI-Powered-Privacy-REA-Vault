{
  "name": "aegis-confidential",
  "version": "1.0.0",
  "description": "AI-Powered Privacy RWA Vault with Chainlink Functions",
  "main": "index.js",
  "scripts": {
    "compile": "npx hardhat compile",
    "test": "npx hardhat test",
    "test-gas": "REPORT_GAS=true npx hardhat test",
    "deploy:local": "npx hardhat run scripts/deploy_vault.js --network hardhat",
    "deploy:sepolia": "npx hardhat run scripts/deploy_vault.js --network sepolia",
    "deploy:mumbai": "npx hardhat run scripts/deploy_vault.js --network mumbai",
    "verify:sepolia": "npx hardhat verify --network sepolia",
    "functions:setup": "npx hardhat run scripts/setup_functions.js --network sepolia",
    "functions:fund": "npx hardhat run scripts/fund_subscription.js --network sepolia",
    "ai:request": "npx hardhat run scripts/request_ai_insight.js --network sepolia",
    "ai:check": "npx hardhat run scripts/check_insight.js --network sepolia",
    "frontend": "cd frontend && npm start",
    "frontend:build": "cd frontend && npm run build",
    "clean": "npx hardhat clean",
    "coverage": "npx hardhat coverage"
  },
  "keywords": ["chainlink", "hardhat", "solidity", "ai", "rwa", "ethereum"],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@nomiclabs/hardhat-ethers": "^2.2.0",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@openzeppelin/contracts": "^4.9.0",
    "@types/chai": "^4.2.0",
    "@types/mocha": "^10.0.0",
    "chai": "^4.2.0",
    "dotenv": "^16.0.0",
    "ethers": "^5.7.0",
    "hardhat": "^2.17.0",
    "hardhat-gas-reporter": "^1.0.0",
    "solidity-coverage": "^0.8.0"
  },
  "dependencies": {
    "@chainlink/contracts": "^0.8.0"
  }
}
